{
  "name": "funs",
  "version": "0.0.1",
  "description": "gave you some useful functions",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build",
    "serve": "preact build && sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "doc": "npx docsify serve docs",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "test": "jest ./tests"
  },
  "dependencies": {
    "moment": "^2.24.0"
  },
  "peerDependencies": {
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@mlz/lint": "^1.1.10",
    "@types/jest": "^25.1.2",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "css-loader": "^1.0.1",
    "docsify": "^4.10.2",
    "docsify-cli": "^4.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "per-env": "^1.0.2",
    "preact": "^10.3.1",
    "preact-cli": "^3.0.0-next.19",
    "preact-jsx-chai": "^3.0.0",
    "preact-markup": "^2.0.0",
    "preact-render-spy": "^1.3.0",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "prettier": "^1.19.1",
    "sirv-cli": "^1.0.0-next.3",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "typings-for-css-modules-loader": "^1.7.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-markdown": "^1.0.2",
    "pretty-quick": "^2.0.1",
    "rollup": "^1.32.0"
  },
  "author": "milo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milobluebell/funs/issues"
  },
  "homepage": "https://github.com/milobluebell/funs#readme",
  "keywords": [
    "javascript",
    "utils",
    "functions"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "style",
          "test",
          "revert",
          "perf",
          "refactor",
          "chore"
        ]
      ],
      "type-empty": [
        2,
        "never"
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        0,
        "never"
      ],
      "subject-case": [
        0,
        "never"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milobluebell/funs.git"
  },
  "main": "index.js"
}
